<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--接口全名-->
<mapper namespace="com.hwadee.springboot.mapper.GoodsMapper">
    <!--与接口方法返回类型中的实体全名一致-->
    <select id="findAll" resultType="com.hwadee.springboot.entity.Goods">
        select  * from t_goods
    </select>
</mapper>